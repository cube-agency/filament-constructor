{
    "name": "cube-agency/filament-constructor",
    "description": "Block constructor for Filament",
    "keywords": [
        "cube-agency",
        "laravel",
        "filament-constructor"
    ],
    "homepage": "https://github.com/cube-agency/filament-constructor",
    "support": {
        "issues": "https://github.com/cube-agency/filament-constructor/issues",
        "source": "https://github.com/cube-agency/filament-constructor"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitrijs Mihailovs",
            "email": "dmitrijs.mihailovs@cube.lv",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/forms": "^4.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.10|^8.1",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^9.0|^10.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "CubeAgency\\FilamentConstructor\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CubeAgency\\FilamentConstructor\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CubeAgency\\FilamentConstructor\\FilamentConstructorServiceProvider"
            ],
            "aliases": {
                "FilamentConstructor": "CubeAgency\\FilamentConstructor\\Facades\\FilamentConstructor"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
